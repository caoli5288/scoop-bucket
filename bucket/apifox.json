{
    "version": "2.2.2",
    "description": "Apifox = Postman + Swagger + Mock + JMeter",
    "homepage": "https://www.apifox.cn/",
    "license": "Freeware",
    "architecture": {
        "64bit": {
            "url": "http://cdn.apifox.cn/download/2.2.2/Apifox-2.2.2.exe#/dl.7z",
            "hash": "sha512:faedcef17269f6edfc18350736e797723838dc99f3731060c43aba495b21f2bed578ef346a29d88cba9623a96dbdedf02335829a4ce2d26c3e903c27b4d4980f",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$*\", \"$dir\\Uninst*\" -Recurse"
            ]
        }
    },
    "shortcuts": [
        [
            "Apifox.exe",
            "Apifox"
        ]
    ],
    "checkver": {
        "url": "http://cdn.apifox.cn/download/latest.yml",
        "regex": "(?sm)version:[\\s]+([\\d.]+).*?http://(?<url>.*?\\.exe)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "http://$matchUrl#/dl.7z",
                "hash": {
                    "url": "http://cdn.apifox.cn/download/latest.yml",
                    "mode": "extract",
                    "regex": "sha512.*?$base64"
                }
            }
        }
    }
}
