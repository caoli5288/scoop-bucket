{
    "version": "2.1.20",
    "description": "Apifox = Postman + Swagger + Mock + JMeter",
    "homepage": "https://www.apifox.cn/",
    "license": "Freeware",
    "architecture": {
        "64bit": {
            "url": "http://cdn.apifox.cn/download/2.1.20/Apifox-2.1.20.exe#/dl.7z",
            "hash": "sha512:db8dff3265f91a434ac919dda5b8d9732af4b1cebe001917f83b88e072b634852c6e6def9e9796a36d3adbd191f1a4ff762c406e372a48dad9d1c11648e11dd2",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
