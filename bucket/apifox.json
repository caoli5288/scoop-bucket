{
    "version": "2.3.10",
    "description": "Apifox = Postman + Swagger + Mock + JMeter",
    "homepage": "https://www.apifox.cn/",
    "license": "Freeware",
    "architecture": {
        "64bit": {
            "url": "http://cdn.apifox.cn/download/2.3.10/Apifox-2.3.10.exe#/dl.7z",
            "hash": "sha512:e555360491eab363e5e30451815359018f47f68746805743c1bfdb24dae6c50e8137bf1b89dc18958a10b39d30a27065e396050d48fc85fd15b5b52ebdb8ec5c",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
