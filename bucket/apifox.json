{
    "version": "2.3.14",
    "description": "Apifox = Postman + Swagger + Mock + JMeter",
    "homepage": "https://www.apifox.cn/",
    "license": "Freeware",
    "architecture": {
        "64bit": {
            "url": "http://cdn.apifox.cn/download/2.3.14/Apifox-2.3.14.exe#/dl.7z",
            "hash": "sha512:0928db5fda87e397a38356651baa6b1335149620ec37ece84ce1155039c81073f49cc5fc7cd0718afe2022d4e671de8ec57f16bb30be4cf41bf6da8ceed08747",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$*\", \"$dir\\Uninst*\" -Recurse"
            ]
        }
    },
    "shortcuts": [
        [
            "Apifox.exe",
            "Apifox"
        ]
    ],
    "checkver": {
