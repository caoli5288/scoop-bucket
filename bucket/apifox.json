{
    "version": "2.2.14",
    "description": "Apifox = Postman + Swagger + Mock + JMeter",
    "homepage": "https://www.apifox.cn/",
    "license": "Freeware",
    "architecture": {
        "64bit": {
            "url": "http://cdn.apifox.cn/download/2.2.14/Apifox-2.2.14.exe#/dl.7z",
            "hash": "sha512:add703d0712c2f8fbe1c91f73be08f787b1f814d463c265d329be920ad28a8d91d35d2602d75d78501610068f9ff91f46dbd79528db98793d043a677fff6d1fc",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
