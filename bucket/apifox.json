{
    "version": "2.3.9",
    "description": "Apifox = Postman + Swagger + Mock + JMeter",
    "homepage": "https://www.apifox.cn/",
    "license": "Freeware",
    "architecture": {
        "64bit": {
            "url": "http://cdn.apifox.cn/download/2.3.9/Apifox-2.3.9.exe#/dl.7z",
            "hash": "sha512:e8d0f01c58e7cdf9fd6a5179c814e3d9c4e7e10b9584d90b25d8decebbc3e1bae6ff90525901ac6a950d440a9ac280e2515f92f7ac4c786935f6ddcb51bcd0e7",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$*\", \"$dir\\Uninst*\" -Recurse"
            ]
        }
    },
    "shortcuts": [
        [
            "Apifox.exe",
            "Apifox"
        ]
    ],
    "checkver": {
        "url": "http://cdn.apifox.cn/download/latest.yml",
        "regex": "(?sm)version:[\\s]+([\\d.]+).*?http://(?<url>.*?\\.exe)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "http://$matchUrl#/dl.7z",
                "hash": {
                    "url": "http://cdn.apifox.cn/download/latest.yml",
                    "mode": "extract",
                    "regex": "sha512.*?$base64"
                }
            }
        }
    }
}
