            "hash": "sha512:7c784e8ce2bd8a5f28f2648e5976a945940c241c7d93b3048aad99c2921e0f40f925fc2fd74dc9553a333296d47661cc5ebbb1b8673ee502d95d4798147fcbbb",
            "pre_install": [
