        "regex": "(?sm)version:[\\s]+([\\d.]+).*?http://(?<url>.*?\\.exe)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "http://$matchUrl#/dl.7z",
                "hash": {
                    "url": "http://cdn.apifox.cn/download/latest.yml",
                    "mode": "extract",
                    "regex": "sha512.*?$base64"
                }
            }
        }
    }
